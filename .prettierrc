{
    "bracketSpacing": true,
    "quoteProps": "as-needed",
    "semi": false,
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 100,
    "bracketSameLine": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "ignore",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "useTabs": false
}
